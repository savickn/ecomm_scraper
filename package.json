  "scripts": {
    "clean": "rimraf ./dist",
    "build-server": "webpack --config ./webpack.config.server.js --env.env=production",
    "build-client": "webpack --config ./webpack.config.client.js --env.env=production",
    "start:dev": "cross-env NODE_ENV=development BABEL_DISABLE_CACHE=1 nodemon ./entry.js",
    "start:prod": "cross-env NODE_ENV=production node ./entry.js",
    "dev": "npm run clean && npm run start:dev",
    "prod": "npm run clean && npm run build-client && npm run build-server && npm run start:prod",
    "start": "node ./src/server.js",
    "start-pm2": "pm2 start ./src/server.js --name fashion-scraper --cron 0 0 20 1/1 * ? *",
    "stop-pm2": "pm2 stop fashion-scraper",
    "scrape": "cross-env NODE_ENV=production node ./src/scripts/run.js",
    "test": "jest"
  },
